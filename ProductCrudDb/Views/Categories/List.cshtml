@model List<Category>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var category in Model){
                <tr>
                <td>@category.Id</td>
                <td>@category.Name</td>
                <td><a href="~/Categories/Edit/@category.Id">Edit</a></td>
                <td><a href="~/Categories/Delete/@category.Id">Delete</a></td>
                </tr>
            }
        </tbody>
    </table>